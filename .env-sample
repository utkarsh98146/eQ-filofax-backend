# SERVER_PORT=

# Port number your server will run on (e.g., 3000)

# SERVER_PORT=

# SECRET_KEY=

# Secret key for your application (generate a strong random string)

# JWT CONFIGURATION

# JWT_SECRET_KEY=

# Secret key for JWT signing (generate a strong random string)

# DATABASE CONFIGURATION

# DATABASE_NAME=

# Name of your database (set this in your database server)

# DATABASE_USER_NAME=

# Username for your database (create this in your database server)

# DATABASE_USER_PASSWORD=

# Password for your database user (set this in your database server)

# DATABASE_SCHEMA_NAME=

# Schema name for your database (optional, depends on your DB setup)

# DATABASE_HOST_NAME=

# Hostname or IP address of your database server (e.g., localhost or cloud DB host)

# DATABASE_PORT_NO=

# Port number your database listens on (e.g., 5432 for PostgreSQL, 3306 for MySQL)

# Google OAuth integration

# GOOGLE_CLIENT_ID=

# Client ID from Google Cloud Console (APIs & Services > Credentials)

# GOOGLE_CLIENT_SECRET=

# Client Secret from Google Cloud Console (APIs & Services > Credentials)

# GOOGLE_REDIRECT_URI=

# OAuth2 redirect URI set in Google Cloud Console

# Microsoft OAuth integration

# MICROSOFT_CLIENT_ID=

# Client ID from Azure Portal (App registrations > Your App > Overview)

# MICROSOFT_CLIENT_SECRET=

# Client Secret from Azure Portal (App registrations > Your App > Certificates & secrets)

# TENANT_ID=

# Directory (tenant) ID from Azure Portal (App registrations > Your App > Overview)

# MICROSOFT_REDIRECT_URI=

# Redirect URI set in Azure Portal (App registrations > Your App > Authentication)

# Front end URL

# FRONTEND_BASE_URL=

# Base URL where your frontend app runs (e.g., http://localhost:3000)

# Zoom integration

# ZOOM_ACCOUNT_ID=

# Account ID from Zoom App Marketplace (App Credentials section)

# ZOOM_CLIENT_ID=

# Client ID from Zoom App Marketplace (App Credentials section)

# ZOOM_CLIENT_SECRET=

# Client Secret from Zoom App Marketplace (App Credentials section)

# SMTP configuration for Google email

# SMTP_HOST=

# SMTP server host (e.g., smtp.gmail.com for Gmail)

# SMTP_PORT=

# SMTP server port (e.g., 465 for SSL, 587 for TLS)

# GOOGLE_EMAIL=

# Your Gmail address used for sending emails

# GOOGLE_APP_PASSWORD=

# App password generated in your Google Account (Security > App passwords)

# Rate Limiting

# RATE_LIMIT_WINDOW_MS=900000

# Time window for rate limiting in milliseconds (e.g., 900000 for 15 minutes)

# RATE_LIMIT_MAX_REQUESTS=100

# Maximum number of requests allowed per window
